{
  "name": "@dorkroom/logic",
  "root": "packages/logic",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "packages/logic",
        "command": "tsc --project tsconfig.lib.json"
      },
      "dependsOn": ["^build"],
      "cache": true,
      "inputs": [
        "production",
        "^production",
        {
          "externalDependencies": ["typescript"]
        }
      ],
      "outputs": ["{projectRoot}/dist"]
    }
  }
}
