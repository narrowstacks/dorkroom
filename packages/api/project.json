{
  "name": "@dorkroom/api",
  "root": "packages/api",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "packages/api",
        "command": "tsc --project tsconfig.lib.json"
      },
      "cache": true,
      "inputs": [
        "production",
        "^production",
        {
          "externalDependencies": ["typescript"]
        }
      ],
      "outputs": ["{projectRoot}/dist"]
    },
    "test": {
      "executor": "@nx/vite:test",
      "outputs": ["{projectRoot}/test-output/vitest"],
      "options": {
        "passWithNoTests": true
      }
    }
  }
}
