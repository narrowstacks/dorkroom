{
  "name": "@dorkroom/api",
  "version": "0.9.0",
  "license": "AGPL-3.0-only",
  "description": "API client and type definitions for the Dorkroom analog photography calculator",
  "keywords": [
    "photography",
    "analog",
    "film",
    "darkroom",
    "api",
    "typescript",
    "film-development",
    "dorkroom"
  ],
  "homepage": "https://dorkroom.art",
  "bugs": {
    "url": "https://github.com/narrowstacks/dorkroom/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/narrowstacks/dorkroom.git",
    "directory": "packages/api"
  },
  "author": "Aaron F. Anderson <aaron@affords.art>",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": ["dist", "!dist/**/*.tsbuildinfo", "README.md", "LICENSE"],
  "sideEffects": false,
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "scripts": {
    "build": "tsc --project tsconfig.lib.json",
    "lint": "biome check .",
    "test": "vitest run --passWithNoTests",
    "typecheck": "tsc --noEmit --project tsconfig.json"
  },
  "devDependencies": {
    "@types/node": "^20.0.0"
  }
