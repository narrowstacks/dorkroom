{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "@dorkroom/dorkroom#build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"],
      "env": [
        "SUPABASE_MASTER_API_KEY",
        "SUPABASE_ENDPOINT",
        "VITE_FEATURE_INFOBASE"
      ]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "test": {
      "dependsOn": ["^build"]
    },
    "typecheck": {
      "dependsOn": ["^build"]
    },
    "lint": {}
  }
}
