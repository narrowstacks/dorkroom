{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": [".env", "tsconfig.json"],
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "@dorkroom/dorkroom#build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"],
      "env": ["SUPABASE_MASTER_API_KEY", "SUPABASE_ENDPOINT"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "test": {
      "dependsOn": ["^build"]
    },
    "typecheck": {
      "dependsOn": ["^build"]
    },
    "lint": {},
    "test:e2e": {
      "dependsOn": ["^build"],
      "cache": false
    }
  }
}
